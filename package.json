{
  "name": "dialog-cl",
  "version": "1.0.6",
  "description": "Flexible JavaScript dialog box",
  "main": "dist/dialog.js",
  "module": "index.js",
  "scripts": {
    "build:dev": "webpack --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
    "build:all": "$npm_execpath run build:dev && $npm_execpath run build:prod",
    "serve": "webpack serve --config webpack.dev.js --open"
  },
  "keywords": [
    "Dialog Box"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/charles-owen/dialog-cl.git"
  },
